{
  "parser": "babel-eslint",
  "plugins": ["@typescript-eslint"],
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "require-jsdoc": "off",
    "quotes": "off",
    "eol-last": "off",
    "linebreak-style": "off",
    "comma-dangle": "off",
    "react/jsx-uses-react": "off",
    "react/jsx-uses-vars": "off"
  },
  "extends": [
    "eslint:recommended",
    "google",
    "plugin:@typescript-eslint/recommended",
    "plugin:react/recommended"
  ]
}
